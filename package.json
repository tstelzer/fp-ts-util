{
  "name": "@tstelzer/fp-ts-util",
  "version": "0.4.13",
  "description": "fp-ts utility",
  "main": "dist/index.js",
  "private": false,
  "sideEffects": false,
  "author": "Timm Stelzer <timmstelzer@gmail.com>",
  "bugs": {
    "url": "https://github.com/tstelzer/fp-ts-util/issues"
  },
  "homepage": "https://github.com/tstelzer/fp-ts-util",
  "repository": {
    "type": "git",
    "url": "https://github.com/tstelzer/fp-ts-util.git"
  },
  "scripts": {
    "lint": "eslint --fix src/**/*.ts",
    "test": "jest --config jest.config.js",
    "verify": "npm run lint && npm run test",
    "build": "rimraf dist && tsc --project ./tsconfig.build.json",
    "prepublishOnly": "npm run build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "./bin/pre-push"
    }
  },
  "keywords": [
    "typescript",
    "fp-ts",
    "io-ts"
  ],
  "license": "MIT",
  "dependencies": {
    "fp-ts": "^2.9.5",
    "io-ts": "^2.2.16",
    "io-ts-types": "^0.5.15"
  },
  "devDependencies": {
    "@types/jest": "^26.0.21",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^5.1.3",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.4",
    "typescript": "^4.2.3"
  }
}
